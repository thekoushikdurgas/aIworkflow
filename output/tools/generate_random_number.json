{
  "name": "generate_random_number",
  "description": "Generate a random number within a specified range",
  "category": "utility",
  "input_parameters": [
    {
      "name": "min_value",
      "type": "integer",
      "description": "Minimum value (inclusive)",
      "required": false
    },
    {
      "name": "max_value",
      "type": "integer",
      "description": "Maximum value (inclusive)",
      "required": false
    }
  ],
  "output_parameters": [
    {
      "name": "random_number",
      "type": "integer",
      "description": "The generated random number"
    },
    {
      "name": "range_info",
      "type": "string",
      "description": "Information about the range used",
      "format": "plain_text"
    },
    {
      "name": "generation_status",
      "type": "string",
      "description": "Status of the number generation",
      "format": "plain_text"
    }
  ],
  "code": "generate_random_number.py",
  "enabled": true
}
